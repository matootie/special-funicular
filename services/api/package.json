{
  "name": "@app/api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "esbuild --bundle --minify --outfile=dist/handler.js --platform=node --sourcemap --define:process.env.NODE_ENV=\\\"production\\\" --tree-shaking=true src/handler.ts",
    "package": "mkdir -p out && zip -j out/api.zip dist/handler.js dist/handler.js.map",
    "develop": "tsup src/main.ts --format cjs --watch --onSuccess \"node dist/main.js\""
  },
  "dependencies": {
    "axios": "^1.3.5",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jose": "^4.14.0",
    "serverless-http": "^3.2.0",
    "superstruct": "^1.0.3",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@app/tsconfig": "*",
    "@types/aws-lambda": "^8.10.114",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.16",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  }
}
